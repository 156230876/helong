{
  "name": "QaTools",
  "version": "1.1.6",
  "manifest_version": 2,
  "description": "QA插件工具集合",
  "default_locale": "zh_CN",
  "icons": {
    "16": "static/images/icon16.png",
    "48": "static/images/icon48.png",
    "128": "static/images/icon128.png"
  },
  "browser_action": {
    "default_icon": "static/images/icon16.png",
    "default_title": "QA插件工具集合",
    "default_popup": "template/qatools_popup.html"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "static/js/core/jquery-1.5.min.js",
      "static/js/options/i18n.js",
      "static/js/options/bg_lib.js",
      "static/js/mod/mod_background.js"
    ]
  },
  "options_page": "template/options/options.html",
  "permissions": [
    "tabs",
    "clipboardRead",
    "contextMenus",
    "cookies",
    "notifications",
    "alarms",
    "webNavigation",
    "storage",
    "\u003Call_urls\u003E"
  ],
  "web_accessible_resources": [
    "static/images/43.png",
    "static/images/baidufe_bj.png",
    "static/images/baidufe_bj.gif",
    "static/images/grid.png",
    "static/images/icon18.png",
    "static/images/icon48.png",
    "static/images/icon128.png",
    "static/images/pbar-ani.gif",
    "static/images/ui-icons_228ef1_256x240.png",
    "static/css/qatools-helper.css",
    "static/css/qatools-jsonformat.css",
    "static/vendor/jquery-ui-1.8/css/jquery-ui-1.8.16.custom.hot_datauri.css",
    "static/css/fe-helper.css",
    "static/img/pbar-ani.gif"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "file://*/*"
      ],
      "js": [
        "static/js/qatools-content.js",

        "static/js/core/jquery-1.5.min.js",
        "static/vendor/jquery-ui-1.8/js/jquery-ui-1.8.11.custom.min.js",

        "static/js/core/core.js",
        "static/js/core/qatools-const.js",
        "static/js/qatools-option.js",

        "static/js/fcp/fcp-fl.js",
        "static/js/fcp/css/fcp-css-analytic.js",
        "static/js/fcp/css/fcp-css.js",
        "static/js/fcp/html/fcp-html-analytic.js",
        "static/js/fcp/html/fcp-html-doctype.js",
        "static/js/fcp/html/fcp-html.js",
        "static/js/fcp/js/fcp-js.js",
        "static/js/fcp/fcp-tabs.js",
        "static/js/fcp/fcp-main.js",

        "static/js/grid/grid-main.js",
        "static/js/qatools-helper.js",
        "static/js/notification/qatools-notification.js",
        "static/js/wpo/qatools-calc-wpo.js",

        "static/js/jsonformat/json-format-dealer.js",
        "static/js/jsonformat/json-format-ent.js",
        "static/js/jsonformat/contentscript-jsonformat.js"
      ],
      "run_at": "document_start",
      "all_frames": false
    },
    {
      "matches":["http://bds.corp.qunar.com/*"],
      "js":["static/js/core/jquery-1.11.1.js","static/js/jenkins/bdshistorysandbox.js"],
      "run_at":"document_idle"
    },
    {
      "matches":["http://bds.corp.qunar.com/*/process*","http://bds.corp.qunar.com/*/qrebuild*","http://qdr.corp.qunar.com/*/*build*"],
      "css": [
        "static/css/button.css"
      ],
      "js":["static/js/jenkins/jenkinsaction.js"],
      "run_at":"document_end"
    },
    {
      "matches":["http://qdr.corp.qunar.com/*/job/delivery*/",
        "http://qdr.corp.qunar.com/job/delivery*/",
        "http://qdr.corp.qunar.com/*/job/room*/",
        "http://qdr.corp.qunar.com/job/room*/"
      ],
      "js":["static/js/core/jquery-1.11.1.js","static/js/jenkins/qdrhistorymake.js"],
      "run_at":"document_idle"
    },
    {
      "matches":["https://user.qunar.com/passport/login.jsp*"],
      "js":["static/js/userqunarcom/useraccount.js"],
      "run_at":"document_end"
    },
    {
      "matches": [ "http://*/*", "https://*/*", "ftp://*/*" ],
      "js": [ "static/js/superdrag/super-drag-and-go.js" ],
      "permissions": [ "tabs" ],
      "run_at": "document_start"
    },
    {
      "matches":["*://net-internals/#dns"],
      "js":["static/js/cleardnscache/cleardnscache.js"],
      "run_at":"document_end"
    },
    {
      "matches":["https://keep.corp.kuaishou.com/*"],
      "css": [
        "static/css/ks/keep/keep.css"
      ],
      "js":["static/js/ks/keep/keep.js"],
      "run_at":"document_end"
    },
    {
      "matches":["https://team.corp.kuaishou.com/*"],
      "js":["static/js/ks/team/team.js"],
      "run_at":"document_end"
    },
    {
      "matches":["http://logger-v2.corp.kuaishou.com/*"],
      "js":[
        "static/js/ks/logger/logger_mod.js"
      ],
      "run_at":"document_idle"
    }
  ],
  "update_url":"http://qa.corp.qunar.com/node/qatools/update",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com 'unsafe-eval'; object-src 'self'",
  "homepage_url": "http://qa.corp.qunar.com"
}
