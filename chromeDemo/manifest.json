{      
    "name": "炒乌鸡",
    "version": "1.0",
    "description": "读取当前标签信息",
    "manifest_version": 2,
    "browser_action": {  
      "default_icon": "icon.png",  
      "default_popup": "popup.html"  
    },  
    "content_security_policy": "style-src 'self' 'unsafe-inline';script-src 'self' 'unsafe-eval' https://cdn.bootcss.com; object-src 'self' ;",
    "permissions": [  
      "tabs","http://*/*","https://*/*"
    ]
}